{
  "name": "pinterest-layout-css-only",
  "version": "1.0.0",
  "description": "Pinterest Layout Using css only",
  "main": "main.js",
  "scripts": {
    "build": "mkdir -p dist && gulp css",
    "preserver": "npm run build",
    "server": "http-server -p 9000 ",
    "prestart": "npm update && gulp",
    "start": "nodemon -e html,json,js,css,scss,sass --ignore coverage --ignore dist --ignore config.js --exec \"npm run server\""
  },
  "author": "Shashi Trivedi",
  "license": "UNLICENSED",
  "devDependencies": {
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean-css": "^2.0.12",
    "gulp-concat": "^2.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "http": "0.0.0",
    "http-server": "^0.9.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "node-reset-scss": "^1.0.1"
  },
  "homepage": "https://sstrivedi.github.io/pinterst-layout-css-only/"
}
